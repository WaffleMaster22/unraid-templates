<?xml version="1.0"?>
<Container version="2">
  <Name>PushNote</Name>
  <Repository>waffle22/pushnote:latest</Repository>
  <Registry/>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project>https://github.com/WaffleMaster22/PushNote</Project>
  <Overview>PushNote is simple webui for any text/URL that you want to send from your PC to your phone or other device as a notification via Pushover.</Overview>
  <Category>Productivity: Tools:</Category>
  <WebUI>http://[IP]:[PORT:5000]</WebUI>
  <TemplateURL/>
  <Icon>https://i.imgur.com/gXsXrXs.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1720265513</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires>Pushover with user key and app key</Requires>
  <Config Name="Port" Target="5000" Default="" Mode="tcp" Description="" Type="Port" Display="always" Required="false" Mask="false">5555</Config>
  <Config Name="PUSHOVER_TOKEN" Target="PUSHOVER_TOKEN" Default="" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false"/>
  <Config Name="PUSHOVER_USER_KEY" Target="PUSHOVER_USER_KEY" Default="" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false"/>
</Container>
