<?xml version="1.0"?>
<Container version="2">
  <Name>PushNote</Name>
  <Repository>waffle22/pushnote</Repository>
  <Registry/>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project>https://github.com/WaffleMaster22/PushNote</Project>
  <Overview>A simple web UI that sends any text you input to a notification on your phone via Pushover.</Overview>
  <Category>Productivity: Tools:</Category>
  <WebUI>http://[IP]:[PORT:5000]</WebUI>
  <TemplateURL/>
  <Icon>https://i.imgur.com/gXsXrXs.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1720296132</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="PUSHOVER_TOKEN" Target="PUSHOVER_TOKEN" Default="" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false"/>
  <Config Name="PUSHOVER_USER_KEY" Target="PUSHOVER_USER_KEY" Default="" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false"/>
  <Config Name="WebUI:" Target="5000" Default="5555" Mode="tcp" Description="" Type="Port" Display="always" Required="false" Mask="false">5550</Config>
</Container>
